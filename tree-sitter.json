{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lovely",
      "camelcase": "Lovely",
      "title": "Lovely",
      "scope": "source.lovely",
      "file-types": [
        "lovely"
      ],
      "injection-regex": "^lovely$",
      "class-name": "TreeSitterLovely"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A low-level language built for the Humble OS",
    "authors": [
      {
        "name": "Miciah Henderson",
        "url": "https://innocencelabs.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/kiahjh/tree-sitter-lovely"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}